# Exemplo de arquivo .env para desenvolvimento
# Copie para .env e ajuste as variáveis conforme necessário

# Ambiente (development ou production)
FLASK_ENV=development

# Chave secreta para sessões (gere uma aleatória para produção)
SECRET_KEY=sua_chave_secreta_aqui

# Porta para desenvolvimento local
PORT=5000

# Configurações de debug
FLASK_DEBUG=True
